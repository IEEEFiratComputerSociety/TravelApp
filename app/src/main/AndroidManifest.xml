<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travel_app_feed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel_app_feed">
        <activity android:name=".View.GezdimPostDetailsActivity"></activity>
        <activity android:name=".View.GezdimActivity"></activity>
        <activity android:name=".View.GezdimFilterCity"></activity>
        <activity android:name=".View.GezdimFilterCategory"></activity>
        <activity android:name=".View.SearchResultActivity"></activity>
        <activity android:name=".View.SearchActivity"></activity>
        <activity android:name=".View.ForgotPasswordActivity"></activity>
        <activity android:name=".View.SignUpActivity"></activity>
        <activity android:name=".View.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>